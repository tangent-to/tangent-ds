{
  "source": {
    "includePattern": ".+\\.js$",
    "excludePattern": "(node_modules|docs|dist|tests)"
  },
  "plugins": [],
  "opts": {
    "encoding": "utf8",
    "recurse": true,
    "access": ["public", "protected"]
  },
  "tags": {
    "allowUnknownTags": true
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}
